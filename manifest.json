{
  "manifest_version": 3,
  "name": "Annotated UUIDs",
  "version": "1.0",
  "description": "Augmented reality for people who regularly look at UUIDs",
  "icons": {
    "32": "images/icon-32.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
